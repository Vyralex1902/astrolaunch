{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "astrolaunch",
  "version": "0.1.0",
  "identifier": "com.astrolaunch.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "AstroLaunch",
        "label": "main",
    "fullscreen": false,
    "resizable": false,
    "decorations": false,
    "transparent": true,
    "alwaysOnTop": true,
    "center": true,
    "width": 800,
    "height": 300,
    "skipTaskbar": true,
    "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
